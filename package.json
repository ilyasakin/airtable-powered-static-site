  "version": "0.1.0",
  "author": "İlyas Akın <ilyas.akin@yahoo.com>",
  "dependencies": {
    "@material-ui/core": "^4.10.2",
    "dotenv": "^8.2.0",
    "gatsby": "^4.25.7",
    "gatsby-image": "^2.4.13",
    "gatsby-plugin-manifest": "^2.4.21",
    "gatsby-plugin-material-ui": "^2.1.9",
    "gatsby-plugin-offline": "^3.2.21",
    "gatsby-plugin-react-helmet": "^3.3.10",
    "gatsby-plugin-sass": "^2.3.12",
    "gatsby-plugin-sharp": "^2.6.22",
    "gatsby-source-airtable": "^2.1.1",
    "gatsby-source-filesystem": "^2.3.23",
    "gatsby-transformer-sharp": "^2.5.11",
    "node-sass": "^4.14.1",
    "prop-types": "^15.7.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-helmet": "^6.1.0"
  },
  "devDependencies": {
    "@types/react-helmet": "^6.0.0",
    "airtable": "^0.8.1",
    "gh-pages": "^3.1.0",
    "prettier": "2.0.5"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "scripts": {
    "build": "yarn gen_tables && gatsby build",
    "develop": "yarn gen_tables && gatsby develop",
    "format": "prettier --write \"**/*.{js,jsx,json,md}\"",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "gen_tables": "node ./src/utils/gen_tables.js",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\" && exit 1",
    "predeploy": "yarn build --prefix-paths",
